{
  "name": "career-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext .js,.jsx,.tsx,.ts .",
    "lint:fix": "eslint src \"src/**/*.{js,jsx,scss,css,json,tsx}\" --report-unused-disable-directives --max-warnings 0 ",
    "preview": "vite preview",
    "prettier:fix": "prettier \"src/**/*.{js,jsx,scss,css,json,tsx}\" --write ",
    "prettier:check": "prettier \"src/**/*.{js,jsx,scss,css,json,tsx}\" --check ",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@mui/styled-engine": "^5.14.15",
    "@mui/system": "^5.14.15",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/react-router-dom": "^5.3.3",
    "react": "^18.2.0",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hook-form-mui": "^6.5.2",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "typescript": "^5.0.2",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@mui/joy": "^5.0.0-beta.12",
    "@mui/utils": "^5.14.15",
    "@types/react": "^18.2.31",
    "@types/react-detect-offline": "^2.4.2",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.1",
    "react-detect-offline": "^2.4.5",
    "react-redux": "^8.1.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.0"
  },
  "lint-staged": {
    "*.{js,jsx,tsx}": "npm run lint:fix",
    "*.{js,jsx,tsx,css,md,html,json}": "npm run prettier:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
